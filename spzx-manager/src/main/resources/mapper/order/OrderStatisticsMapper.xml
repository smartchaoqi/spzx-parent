<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aqiu.spzx.manager.mapper.OrderStatisticsMapper">

    <insert id="save">
        insert into order_statistics (
            id,
            order_date,
            total_amount,
            total_num
        ) values (
                     #{id},
                     #{orderDate},
                     #{totalAmount},
                     #{totalNum}
                 )
    </insert>
</mapper>